{
  "name": "rehype-shiki",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "author": "Raqueebuddin Aziz",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*/index.d.ts",
      "import": "./dist/*/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*/index.d.ts",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown --out-dir ./dist",
    "dev": "npm run build -- --watch src",
    "lint": "eslint --fix src",
    "format": "prettier --write src"
  },
  "devDependencies": {
    "@types/hast": "^3.0.4",
    "@types/node": "latest",
    "tsdown": "^0.15.7",
    "typescript": "^5"
  },
  "dependencies": {
    "hast-util-from-html": "^2.0.3",
    "unist-util-visit": "^5.0.0"
  }
}
