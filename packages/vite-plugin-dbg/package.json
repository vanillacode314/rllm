{
  "name": "vite-plugin-dbg",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "author": "Raqueebuddin Aziz",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*/index.d.ts",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown --out-dir ./dist",
    "dev": "npm run build -- --watch src",
    "lint": "eslint --fix src",
    "format": "prettier --write src"
  },
  "dependencies": {
    "@babel/core": "^7.28.4",
    "@babel/generator": "^7.28.3",
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "@babel/types": "^7.28.4"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "eslint": "^9.38.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.9",
    "typescript": "^5.9.3"
  }
}
